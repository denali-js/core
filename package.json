{
  "name": "denali",
  "version": "0.0.3",
  "description": "An opinionated Node framework for building robust JSON APIs",
  "homepage": "https://github.com/davewasmer/denali",
  "repository": "davewasmer/denali",
  "author": {
    "name": "Dave Wasmer",
    "email": "davewasmer@gmail.com",
    "url": "davewasmer.com"
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "denali": "bin/denali"
  },
  "main": "dist/index.js",
  "keywords": [
    "jsonapi",
    "framework",
    "resources",
    "opinionated",
    "denali"
  ],
  "peerDependencies": {
    "node-inspector": "0.12.x"
  },
  "devDependencies": {
    "axios": "^0.8.1",
    "body-parser": "^1.14.2",
    "coveralls": "^2.11.4",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.2",
    "faker": "^3.0.1",
    "in-publish": "2.0.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "mock-fs": "^3.6.0",
    "mockery": "^1.4.0",
    "must": "^0.13.1"
  },
  "scripts": {
    "prepublish": "in-publish && npm run lint && npm test && npm run coverage && npm run compile || npm run build",
    "test": "NODE_ENV=test mocha --compilers js:babel/register",
    "lint": "eslint lib test",
    "build": "which babel && npm run clean && cp -r lib/ dist && babel lib --out-dir dist --ignore 'cli/blueprints/*/files'",
    "coverage": "babel-node isparta cover _mocha && coveralls < coverage/lcov.info",
    "postcoverage": "rimraf coverage",
    "watch": "watch 'npm run build && npm run test' lib test",
    "clean": "rimraf dist/*"
  },
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.34",
    "blackburn": "0.0.2",
    "bluebird": "^2.10.0",
    "broccoli": "^0.16.9",
    "broccoli-babel-transpiler": "^5.5.0",
    "broccoli-file-creator": "^1.0.1",
    "broccoli-funnel": "^1.0.1",
    "broccoli-lint-eslint": "^1.1.1",
    "broccoli-slow-trees": "^2.0.0",
    "broccoli-stew": "^1.2.0",
    "broccoli-uglify-sourcemap": "^1.1.0",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "copy-dereference": "^1.0.0",
    "core-object": "^1.1.0",
    "cors": "^2.7.1",
    "dag-map": "^1.0.2",
    "diveSync": "^0.3.0",
    "express": "^4.13.3",
    "express-force-ssl": "^0.3.0",
    "express-request-id": "^1.1.0",
    "findup": "^0.1.5",
    "findup-sync": "^0.3.0",
    "foraker": "0.0.4",
    "glob": "^6.0.1",
    "helmet": "^1.0.1",
    "http-errors": "^1.3.1",
    "inflection": "^1.8.0",
    "liftoff": "^2.2.0",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "nodemon": "^1.8.1",
    "nsp": "^2.2.0",
    "qs": "^6.0.1",
    "require-all": "^2.0.0",
    "require-dir": "^0.3.0",
    "resolve": "^1.1.6",
    "response-time": "^2.3.1",
    "rimraf": "^2.4.2",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.2",
    "trash": "^3.4.0"
  }
}
